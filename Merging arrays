#include<stdio.h>
int main()
{
	int a[10],b[10],c[10],i,n1,n2,n3;
		printf("name:tejaswi\nreg no-192372120\n");
	printf("size of 1st array:");
	scanf("%d",&n1);
	printf("enter elements:");
	for(i=0;i<n1;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("size of 2nd array:");
	scanf("%d",&n2);
	printf("enter elements:");
	for(i=0;i<n2;i++)
	{
		scanf("%d",&b[i]);
	}
	n3=n1+n2;
	for(i=0;i<n1;i++)
	{
		c[i]=a[i];
	}
	for(i=0;i<n2;i++)
	{
		c[n1+i]=b[i];
	}
	printf("merge array:");
	for(i=0;i<n3;i++)
	{
		printf("%d",c[i]);
		printf("\t");
	}
	return 0;
}
