#include<stdio.h>
int main() 
{ 
    int arr[] = {20, 30, 10, 2, 10, 20, 30, 11}; 
    int n = sizeof(arr)/sizeof(arr[0]); 

    int a[n];
 
    for(int i=0; i < n; i++){
        
        if(a[i] == 0){
            int count = 1;
          
            for(int j = i+1; j < n; j++) { 
				if(arr[i] == arr[j]) 
				{
					count++; 
					a[j] = 1; 
				} 
			}  
			
            if(count > 1)
            printf("name:tejaswi\nregno:192372120\n");
                printf("%d ",arr[i]);
        }
    }
   return 0; 
}
